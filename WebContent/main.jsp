<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
<link rel="stylesheet" href="assets/css/main.css" />
</head>
<body>
		<!-- One -->
			<section id="one">
				<div class="inner">
					<header>
						<h2>안녕하세요 여러분</h2>
					</header>
					<p>반가워요 ? </p>
					<ul class="actions">
						<li><a href="#" class="button alt">더보실래요</a></li>
					</ul>
				</div>
			</section>

		<!-- Two -->
			<section id="two">
				<div class="inner">
					<article>
						<div class="content">
							<header>
								<h3>이런것도</h3>
							</header>
							<div class="image fit">
								<img src="images/wall2.jpg" alt="" />
							</div>
							<p>야호</p>
						</div>
					</article>
					<article class="alt">
						<div class="content">
							<header>
								<h3>있어요</h3>
							</header>
							<div class="image fit">
								<img src="images/architecture.jpg" alt="" />
							</div>
							<p>만세</p>
						</div>
					</article>
				</div>
			</section>
			
		<!-- Banner -->
			<section id="banner">
				<div class="inner">
					<h1>콘크리트</h1>
					<ul class="actions">
						<li><a href="#" class="button alt">멋져요</a></li>
					</ul>
				</div>
			</section>
			
		<!-- Footer -->
			<section id="footer">
				<div class="inner">
					<header>
						<h2>Get in Touch</h2>
					</header>
					<form method="post" action="mailFunction">
						<div class="field half first">
							<label for="name">Name</label>
							<input type="text" name="name" id="name" />
						</div>
						<div class="field half first">
							<label for="email">Email</label>
							<input type="text" name="email" id="email" />
						</div>
						<div class="field">
							<label for="message">Message</label>
							<textarea name="message" id="message" rows="6"></textarea>
						</div>
						<ul class="actions">
							<li><input type="submit" value="Send Message" class="alt" /></li>
						</ul>
					</form>
					<div class="copyright">
						&copy; Untitled Design: <a href="https://templated.co/">TEMPLATED</a>. Images <a href="https://unsplash.com/">Unsplash</a>
					</div>
				</div>
			</section>
</body>
</html>